{
    "name": "LinkedIn video dowload",
    "description" : "powered by Mosin Andrii",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["storage","activeTab"],
    "content_scripts": [{
          "matches": ["https://www.linkedin.com/*"],
          "run_at": "document_idle",
          "js": ["content.js"]
        }],
        "browser_action": {
          "default_icon": "img/linkedin.png"
        }
        
  }